<?xml version="1.0" encoding="utf-8"?>
<resources>
<style name="notificationIcon">
	<item name="android:layout_marginLeft">2sp</item>
	<item name="android:src">@drawable/notification_icon_status_bar</item>
</style>
<style name="notificationTime">
	<item name="android:layout_marginLeft">3sp</item>
	<item name="android:textColor">#000</item>
	<item name="android:textSize">12sp</item>
</style>
<style name="notificationUsername" parent="notificationTime">
	<item name="android:layout_marginLeft">4sp</item>
	<item name="android:textStyle">bold</item>
</style>
<style name="notificationIsMessage" parent="notificationTime">
	<item name="android:layout_marginLeft">4sp</item>
	<item name="android:text">@string/notification_is_message</item>
	<item name="android:visibility">gone</item>
</style>
<style name="notificationText">
	<item name="android:textColor">#000</item>
</style>

</resources>
